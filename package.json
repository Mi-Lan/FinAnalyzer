{
  "name": "finanalyzer",
  "version": "0.1.0",
  "description": "AI-powered financial analysis platform with automated data ingestion and scoring",
  "private": true,
  "scripts": {
    "update-progress": "node scripts/update-readme-progress.js",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "build": "turbo run build",
    "clean": "rm -rf node_modules */node_modules .next */dist",
    "prepare": "husky || echo 'Husky not installed yet'"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "dotenv-cli": "^8.0.0",
    "husky": "^9.1.7",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mi-Lan/FinAnalyzer.git"
  },
  "keywords": [
    "financial-analysis",
    "ai",
    "task-master",
    "nextjs",
    "fastapi",
    "postgresql",
    "langchain"
  ],
  "author": "Milan",
  "license": "MIT",
  "engines": {
    "node": ">=20.11.0",
    "pnpm": ">=10.2.0"
  },
  "packageManager": "pnpm@10.2.0"
}
